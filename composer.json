{
    "name": "dadyday/accepter",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "demo": "\"./vendor/bin/tester\" -C demo",
        "test": "tests\\test.cmd",
        "cover": "\"./vendor/bin/tester\" -C -j 4 --stop-on-fail --setup tests/setup.php --coverage tests/_coverage.html --coverage-src src tests"
    },
    "autoload": {
        "psr-4": {
            "Accepter\\": "src/"
        }
    },
    "require": {
        "facebook/webdriver": "^1.5",
        "tracy/tracy": "^2.4",
        "nette/utils": "^2.5"
    },
    "require-dev": {
        "Nette/Tester": "^2.0"
    }
}
